<UserControl x:Class="SimpleCoinTrading.Wpf.Sample2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SimpleCoinTrading.Wpf"
             mc:Ignorable="d"
             d:DesignHeight="800" d:DesignWidth="1200"
             d:DesignInstance="{d:DesignInstance local:Sample2ViewModel}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="200"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        
        <local:Algorithm DataContext="{Binding AlgorithmViewModel}"/>
        
        <local:Logs Grid.Row="0" Grid.Column="1" DataContext="{Binding LogViewModel}"/>
    </Grid>
</UserControl>
